  print("${num} * ${i} = ${num*i}");
}
}

Q9:
ANS:
Code:
void main(){
   var a=5;
  var b=3;
  // for addition
  var sumplus=a+b;
  print(sumplus);
   // for subtraction
  var sumsub=a-b;
  print(sumsub);
   // for multiplication
  var summul=a*b;
  print(summul);
   // for division
  var sumdiv=a/b;
  print(sumdiv);
   // for addition
  var sumrem=a%b;
  print(sumrem);
}

Q10:
ANS:
cODE:
void main(){
   String string='i';
  if(string=='a'){
    print("it is vowel");
  }
  else if (string=="u"){
    print("it is vowel");
  }
  else if (string=="e"){
    print("it is vowel");
  }
  else if (string=="i"){
    print("it is vowel");
  }
  else if (string=="o"){
    print("it is vowel");
  }
  else{
    print(false);
  }
}
  

